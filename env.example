# Quote Management Service Environment Variables
# Copy this file to .env and modify the values for your environment

# TMForum API Configuration
TMFORUM_API_BASE_URL=https://an-dhub-sbx.dome-project.eu/tmf-api

# Alternative TMForum API URLs (uncomment and modify as needed)
# TMFORUM_API_BASE_URL=https://dome-dev.eng.it/tmf-api
# TMFORUM_API_BASE_URL=https://tmf.dome-marketplace-sbx.org/tmf-api

# Application Configuration
SPRING_PROFILES_ACTIVE=local

# JVM Configuration for Docker
JAVA_OPTS=-Xmx512m -Xms256m

# Port Configuration (change if needed to avoid conflicts)
APP_PORT=8080

# Logging Level (DEBUG, INFO, WARN, ERROR)
LOG_LEVEL=INFO 